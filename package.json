{
    "name": "h5p-nodejs-library",
    "version": "v0.0.10",
    "description": "",
    "repository": {
        "type": "git",
        "url": "https://github.com/Lumieducation/h5p-nodejs-library"
    },
    "scripts": {
        "start": "npm run dev",
        "clean": "rm -rf build && rm -rf dist && rm -rf tmp && rm -rf h5p/content/* && rm -rf h5p/libraries/*",
        "uninstall": "npm run clean && rm -rf node_modules",
        "dev": "npm run build && node ./build/boot.js",
        "build": "npm run lint  && ./node_modules/.bin/tsc --project ./tsconfig.json",
        "lint": "./node_modules/.bin/tslint --project ./tsconfig.json --config tslint.json"
    },
    "author": "Jan Philip Schellenberg",
    "dependencies": {
        "body-parser": "^1.18.2",
        "compression": "^1.7.1",
        "copy-dir": "^0.4.0",
        "debug": "2.3.3",
        "express": "^4.16.0",
        "express-fileupload": "^0.4.0",
        "express-react-views": "^0.10.5",
        "lodash": "^4.17.11",
        "mkdirp": "^0.5.1",
        "mv": "^2.1.1",
        "promise-queue": "^2.2.5",
        "react": "16.4.1",
        "react-dom": "16.4.1",
        "recursive-readdir": "^2.2.2",
        "rimraf": "^2.6.2",
        "unzipper": "^0.9.4",
        "url-parse": "^1.4.3"
    },
    "devDependencies": {
        "@types/express": "4.0.37",
        "@types/express-serve-static-core": "4.0.49",
        "@types/lodash": "^4.14.118",
        "@types/react": "16.4.18",
        "@types/node": "^10.12.9",
        "@types/react-dom": "0.14.20",
        "tslint": "^5.7.0",
        "tslint-config-airbnb": "^5.3.1",
        "tslint-config-prettier": "^1.6.0",
        "tslint-react": "3.2.0",
        "typescript": "^3.1.3"
    },
    "main": "./build/router.js",
    "types": "./src/types.d.ts"
}
